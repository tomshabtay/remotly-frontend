<div class="page-header">
<h3>Add Device</h3>
<p>Enter properties for the new device</p>
</div>
<div class="addDevice">


<form class="formAddDevice" role="form"
  action="/process-add-device?form=adddevice" method="POST">
 
  <input type="hidden" name="_csrf" value="{{csrf}}">
 
  <div class="form-group" style="padding:20px;">
    <label for="fieldName" class="col-sm-2 control-label">
    Name:</label>
    <div class="col-sm-4">
      <input type="text" class="form-control" id="fieldName"
      name="name">
    </div>
  </div> <!-- End of form-group -->

 
  <div class="form-group" style="padding:20px;">
    <label for="fieldIp" class="col-sm-2 control-label">
    Ip:</label>
    <div class="col-sm-4">
      <input type="text" class="form-control" id="fieldIp"
      name="ip">
    </div>
  </div> <!-- End of form-group -->


  <div class="form-group" style="padding:20px;">
    <label for="fieldUsername" class="col-sm-2 control-label">
    Username:</label>
    <div class="col-sm-4">
      <input type="text" class="form-control" id="fieldUsername"
      name="username">
    </div>
  </div> <!-- End of form-group -->


    <div class="form-group" style="padding:20px;">
    <label for="fieldPassword" class="col-sm-2 control-label">
    Password:</label>
    <div class="col-sm-4">
      <input type="text" class="form-control" id="fieldPassword"
      name="password">
    </div>
  </div> <!-- End of form-group -->


 

  <div class="form-group" style="padding:20px;">
    <div class="col-sm-4">
      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  </div> <!-- End of form-group -->

<br>
 


</form>
 
</div> <!-- End of contactUs -->